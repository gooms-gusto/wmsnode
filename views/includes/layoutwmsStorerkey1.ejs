<% dataInfo.forEach(element => { %>
    <form action="/wmsstorerkey" method="POST">

        <div class="form-row">
            <div>
                <input name="txt_id" type="hidden" value="<%= element._id %> ">
            </div>
            <div class="col-md-4 mb-3">
                <label for="StorerKey">Storerkey</label>
                <input class="form-control" name="txtstorerkey" type="text" placeholder="StorerKey" value="<%= element.storerkey %> " required=" ">
            </div>
        </div>
        <div class="form-row ">
            <div class="col-md-4 mb-3 ">
                <label for="StorerName ">StorerName</label>
                <input class="form-control " name="txtstorername" type="text " placeholder="StorerName " value="<%=element.storername %> " required=" ">
            </div>
        </div>
        <button class="btn btn-primary" type="submit">Update</button>
        <input type="button" onclick="Backform()" class="btn btn-primary" value="Back"></input>
    </form>
    <% }) %>